{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "ng build",
        "outputDirectory": "dist/portfolio/browser"
      }
    }
  ],
  "routes": [
    {
      "src": "/(assets|favicon.ico)/(.*)",
      "dest": "/assets/$2"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}